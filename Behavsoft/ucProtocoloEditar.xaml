<Window x:Class="Behavsoft.ucProtocoloEditar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Editar Protocolo" Height="360" Width="400" ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <ComboBox x:Name="cbTipoComportamento" Margin="5,10,5,0" VerticalAlignment="Top"
                      HorizontalAlignment="Stretch" DisplayMemberPath="Nome" SelectionChanged="cbTipoComportamento_SelectionChanged" />
        
        <Button Name="btnEditar" Width="80" Height="25" Content="Editar" Margin="5,37,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Click="btnEditar_Click" />
        
        <GroupBox x:Name="gbTeclas" Header="Teclas" Margin="5,67,5,0" VerticalAlignment="Top" Height="228" HorizontalAlignment="Stretch" IsEnabled="False">
            <Grid>
                <TextBlock x:Name="tbTeclaA" Margin="5,10,0,0" HorizontalAlignment="Left" Height="18" Text="A" VerticalAlignment="Top" Width="10" Style="{StaticResource TeclaAtalhoNormal}" />
                <TextBox x:Name="txtTeclaA" Height="23" Margin="20,7,5,0" HorizontalAlignment="Stretch" Text="" VerticalAlignment="Top" />

                <TextBlock x:Name="tbTeclaS" Margin="5,35,0,0" HorizontalAlignment="Left" Height="18" Text="S" VerticalAlignment="Top" Width="10" Style="{StaticResource TeclaAtalhoNormal}" />
                <TextBox x:Name="txtTeclaS" Height="23" Margin="20,32,5,0" HorizontalAlignment="Stretch" Text="" VerticalAlignment="Top" />

                <TextBlock x:Name="tbTeclaD" Margin="5,60,0,0" HorizontalAlignment="Left" Height="18" Text="D" VerticalAlignment="Top" Width="10" Style="{StaticResource TeclaAtalhoNormal}" />
                <TextBox x:Name="txtTeclaD" Height="23" Margin="20,57,5,0" HorizontalAlignment="Stretch" Text="" VerticalAlignment="Top" />

                <TextBlock x:Name="tbTeclaF" Margin="5,85,0,0" HorizontalAlignment="Left" Height="18" Text="F" VerticalAlignment="Top" Width="10" Style="{StaticResource TeclaAtalhoNormal}" />
                <TextBox x:Name="txtTeclaF" Height="23" Margin="20,82,5,0" HorizontalAlignment="Stretch" Text="" VerticalAlignment="Top" />

                <TextBlock x:Name="tbTeclaH" Margin="5,110,0,0" HorizontalAlignment="Left" Height="18" Text="H" VerticalAlignment="Top" Width="10" Style="{StaticResource TeclaAtalhoNormal}" />
                <TextBox x:Name="txtTeclaH" Height="23" Margin="20,107,5,0" HorizontalAlignment="Stretch" Text="" VerticalAlignment="Top" />

                <TextBlock x:Name="tbTeclaJ" Margin="5,135,0,0" HorizontalAlignment="Left" Height="18" Text="J" VerticalAlignment="Top" Width="10" Style="{StaticResource TeclaAtalhoNormal}" />
                <TextBox x:Name="txtTeclaJ" Height="23" Margin="20,132,5,0" HorizontalAlignment="Stretch" Text="" VerticalAlignment="Top" />

                <TextBlock x:Name="tbTeclaK" Margin="5,160,0,0" HorizontalAlignment="Left" Height="18" Text="K" VerticalAlignment="Top" Width="10" Style="{StaticResource TeclaAtalhoNormal}" />
                <TextBox x:Name="txtTeclaK" Height="23" Margin="20,157,5,0" HorizontalAlignment="Stretch" Text="" VerticalAlignment="Top" />

                <TextBlock x:Name="tbTeclaL" Margin="5,185,0,0" HorizontalAlignment="Left" Height="18" Text="L" VerticalAlignment="Top" Width="10" Style="{StaticResource TeclaAtalhoNormal}" />
                <TextBox x:Name="txtTeclaL" Height="23" Margin="20,182,5,0" HorizontalAlignment="Stretch" Text="" VerticalAlignment="Top" />
            </Grid>
        </GroupBox>

        <Button Name="btnSalvar" Width="80" Height="25" Content="Salvar" Margin="5,300,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" IsEnabled="False" Click="btnSalvar_Click" />
        <Button Name="btnCancelar" Width="80" Height="25" Content="Cancelar" Margin="95,300,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" IsEnabled="False" Click="btnCancelar_Click" />
    </Grid>
</Window>
